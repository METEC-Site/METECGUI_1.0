{
  "version": 1,
  "formatters": {
    "simple": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "toConsole": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "simple",
      "stream": "ext://sys.stdout"
    },
    "postLogger": {
      "class": "logging.FileHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "filename":"ArchivePostLogger.log",
      "mode": "a"
    }
  },
  "loggers": {
    "thisFileLogger": {
      "level": "DEBUG",
      "handlers": [
        "postLogger"
      ]
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": [
      "toConsole"
    ]
  },
  "disable_existing_loggers": false
}