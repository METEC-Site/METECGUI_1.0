{
  "version": 1,
  "formatters": {
    "simple": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "stream": "ext://sys.stdout"
    },
    "archiveLogger": {
      "()": "Framework.Archive.Logger.getLogger",
      "level": "DEBUG"
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": [
      "console",
      "archiveLogger"
    ]
  }
}